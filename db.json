      "title": "React Patterns Quiz",
      "questions": [
        {
          "id": "q1",
          "question": "What is a Higher Order Component (HOC)?",
          "options": [
            "A component with high priority",
            "A function that takes a component and returns a new component",
            "A component that renders other components",
            "A component with complex logic"
          ],
          "correctAnswer": "A function that takes a component and returns a new component"
        },
        {
          "id": "q2",
          "question": "What is the render prop pattern?",
          "options": [
            "Rendering props as components",
            "A technique for sharing code using a prop whose value is a function",
            "Rendering multiple props",
            "A way to optimize rendering"
          ],
          "correctAnswer": "A technique for sharing code using a prop whose value is a function"
        },
        {
          "id": "q3",
          "question": "What is React Context used for?",
          "options": [
            "Component styling",
            "Passing data through component tree without props",
            "Event handling",
            "Component lifecycle"
          ],
          "correctAnswer": "Passing data through component tree without props"
        },
        {
          "id": "q4",
          "question": "What is the compound component pattern?",
          "options": [
            "Using multiple components together",
            "A pattern where components work together to form a complete UI",
            "Combining different libraries",
            "Creating complex animations"
          ],
          "correctAnswer": "A pattern where components work together to form a complete UI"
        },
        {
          "id": "q5",
          "question": "What is React.memo used for?",
          "options": [
            "Memorizing values",
            "Preventing unnecessary re-renders",
            "Creating memos",
            "Memory management"
          ],
          "correctAnswer": "Preventing unnecessary re-renders"
        },
        {
          "id": "q6",
          "question": "What is the custom hook pattern?",
          "options": [
            "Hooks provided by React",
            "User-defined functions that use React hooks",
            "Third-party hooks",
            "Deprecated hooks"
          ],
          "correctAnswer": "User-defined functions that use React hooks"
        },
        {
          "id": "q7",
          "question": "What is prop drilling?",
          "options": [
            "Creating holes in props",
            "Passing props through multiple component layers",
            "Optimizing prop performance",
            "Validating props"
          ],
          "correctAnswer": "Passing props through multiple component layers"
        },
        {
          "id": "q8",
          "question": "What is the provider pattern in React?",
          "options": [
            "A component that provides data to its children",
            "Internet service provider",
            "Data provider service",
            "Component provider library"
          ],
          "correctAnswer": "A component that provides data to its children"
        },
        {
          "id": "q9",
          "question": "What is React.forwardRef used for?",
          "options": [
            "Forwarding emails",
            "Passing refs through components",
            "Creating references",
            "Forwarding props"
          ],
          "correctAnswer": "Passing refs through components"
        },
        {
          "id": "q10",
          "question": "What is the controlled component pattern?",
          "options": [
            "Components with controlled access",
            "Form inputs controlled by React state",
            "Components with limited functionality",
            "Restricted components"
          ],
          "correctAnswer": "Form inputs controlled by React state"
        }
      ]
